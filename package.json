{
  "name": "@helium-pay/wallet-extension",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "yarn lint:fix && craco build",
    "build:dev": "watch 'REACT_APP_OPTIMISE=false DISABLE_ESLINT_PLUGIN=true craco build' ./src",
    "serve": "serve -d build",
    "sync:android": "env-cmd -f .env.staging.local craco build && env-cmd -f .env.staging.local npx cap sync",
    "sync:prod:android": "env-cmd -f .env.production.local craco build && env-cmd -f .env.production.local npx cap sync",
    "build:android": "yarn sync:android && cd android && bundle exec fastlane buildApk --env staging",
    "build:prod:android": "yarn sync:prod:android && cd android && bundle exec fastlane buildApk --env production",
    "deploy:android": "yarn build:android && cd android && bundle exec fastlane internal --env staging",
    "deploy:prod:android": "yarn build:prod:android && cd android && bundle exec fastlane internal --env production",
    "serve:android": "npx cap open android",
    "start:dev": "DISABLE_ESLINT_PLUGIN=true PORT=3001 craco start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/nextjs-app.eslintcache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/clipboard": "5.0.4",
    "@capacitor/core": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@ionic/react": "6.6.2",
    "@ionic/react-router": "6.6.2",
    "history": "4.10.1",
    "i18next": "22.4.15",
    "ionicons": "6.1.3",
    "lodash": "4.17.21",
    "qrcode.react": "3.1.0",
    "react-countdown-circle-timer": "3.2.1",
    "react-i18next": "12.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "react-virtuoso": "4.3.8",
    "swiper": "9.2.4",
    "swr": "1.3.0",
    "typescript": "4.8.4",
    "web-vitals": "0.2.4",
    "workbox-background-sync": "5.1.4",
    "workbox-broadcast-update": "5.1.4",
    "workbox-cacheable-response": "5.1.4",
    "workbox-core": "5.1.4",
    "workbox-expiration": "5.1.4",
    "workbox-google-analytics": "5.1.4",
    "workbox-navigation-preload": "5.1.4",
    "workbox-precaching": "5.1.4",
    "workbox-range-requests": "5.1.4",
    "workbox-routing": "5.1.4",
    "workbox-strategies": "5.1.4",
    "workbox-streams": "5.1.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@craco/craco": "7.1.0",
    "@craco/types": "7.1.0",
    "@helium-pay/eslint-config-bases": "workspace:",
    "@types/lodash": "4.14.195",
    "@types/node": "12.20.55",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "env-cmd": "10.1.0",
    "eslint": "8.25.0",
    "serve": "14.2.0",
    "watch": "1.0.2"
  },
  "browserslist": {
    "production": [
      "Chrome >=60",
      "ChromeAndroid >=60",
      "Firefox >=63",
      "Edge >=79",
      "Safari >=13",
      "iOS >=13"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
